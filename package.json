{
  "name": "blyss-playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest tests --coverage --maxWorkers=10"
  },
  "dependencies": {
    "@blyss/sdk": "^0.1.6",
    "@clerk/nextjs": "^4.23.5",
    "@mantine/core": "^6.0.1",
    "@mantine/hooks": "^6.0.1",
    "@mantine/next": "^6.0.1",
    "@tabler/icons-react": "^2.4.0",
    "@vercel/analytics": "^0.1.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^8.0",
    "helmet": "^6.0.1",
    "highlight.js": "^11.7.0",
    "jwks-rsa": "^3.0.0",
    "lodash": "^4.17.21",
    "next": "^13.1.0",
    "nodemon": "^2.0.20",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.8.5",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-syntax-highlighter": "^15.5.5",
    "cypress": "^11.2.0",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.8.0",
    "start-server-and-test": "^1.15.1",
    "typescript": "4.9.5"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "rootDir": "./",
    "moduleNameMapper": {
      ".(css)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupFiles": [
      "./tests/environment.js"
    ],
    "setupFilesAfterEnv": [
      "./tests/setup.js"
    ],
    "collectCoverageFrom": [
      "components/**/*.*",
      "pages/**/*.*"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ]
  }
}
